@using EntityLayer1.Concrete
@model List<Heading>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Index</h2>
<br />
<table class="table table-bordered">
    <tr>
        <td>ID</td>
        <td>BAŞLIK</td>
        <td>KATEGORİ</td>
    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>@item.HeadingID</td>
    <td>@item.HeadingName</td>
    <td>@item.Category.CategoryName</td>
</tr>
    }
</table>
